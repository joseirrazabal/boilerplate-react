	"scripts": {
		"prod:env": "cp .env_example .env",
		"prod:copy": "cp -R .env package.json package-lock.json public dist",
		"prod:npm": "npm i --only=prod",
		"prod:run": "cross-env NODE_ENV=production node app.js",
		"build:app": "babel kit/apollo/apollo-server-middleware.js -d dist/apollo/ && babel kit/app.js kit/webpack/middleware.js -d dist/",
		"build": "cross-env NODE_ENV=production LOCAL=true webpack --config kit/webpack/index.js && npm run build:app",
		"build:stats": "cross-env NODE_ENV=production LOCAL=true webpack --config kit/webpack/index.js -stats",
		"build:prod": "npm run prod:env && npm run build && npm run prod:copy && cd dist && npm run prod:npm",
		"build:all": "npm run build && npm run prod:copy && cd dist && npm run prod:npm",
		"prod": "npm run build && npm run prod:copy && cd dist && npm run prod:npm && npm run prod:run",
		"prod:start": "cross-env NODE_ENV=production babel-node kit/app.js",
		"start": "cross-env NODE_ENV=development babel-node --trace-deprecation kit/app.js",
		"debug": "babel-node --inspect-brk kit/app.js",
		"precommit": "npm run version",
		"version": "node version.js && git add package.json ",
		"test": "jest",
		"lint": "eslint src || true",
		"lint:fix": "eslint --fix src || true"
	},
	"optionalDependencies": {
